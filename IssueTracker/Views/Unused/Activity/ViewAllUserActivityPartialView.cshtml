@model IEnumerable<IssueTracker.Models.ActivityModel>

<h2>Activity</h2>
<table class="table" id="userActivityTable">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ActivityContent)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DateTimeCreated)
            </th>
        </tr>
    </thead>

    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.ActivityContent)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.DateTimeCreated)
                </td>
            </tr>
        }
    </tbody>

</table>
